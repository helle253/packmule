<h>Update your admin credentials:</h>
<%= form_with url: admin_credentials_path(@administrator), model: @administrator do |form| %>
  <table>
    <tr>
      <td><%= form.label :email, "Email (Optional):" %></td>
      <td><%= form.text_field :email%></td>
    </tr>
    <tr>
      <td><%= form.label :password, "Password:" %></td>
      <td><%= form.password_field :password %></td>
    </tr>
    <tr>
      <td><%= form.label :password_confirmation, "Confirm Password:" %></td>
      <td><%= form.password_field :password_confirmation %></td>
    </tr>
  </table>
  <%= form.submit %>
<% end %>
